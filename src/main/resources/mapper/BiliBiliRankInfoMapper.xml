<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.justep.crawlerbilibili.mapper.BiliBiliRankInfoMapper">
<insert id="insertRankInfo" parameterType="pers.justep.crawlerbilibili.bean.BiliBiliRankInfoPojo">
INSERT INTO crawlerbilibili_rank_info (rank,title,videoUrl,play,danmaku,author,authorUrl,score,type,subType,contributeType,termType,time)
